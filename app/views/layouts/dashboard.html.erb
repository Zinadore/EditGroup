<!DOCTYPE html>
<html>
<head>
  <title>EDITGroupWebsite</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'admin/dashboard', 'data-turbolinks-track' => true  %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'admin', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="nav-md">

  <div class="container body">
    <div class="main_container">

      <%= render 'side_menu'%>
      <%= render 'header' %>

      <div class="right_col" role="main" id="right-col-id">
        <div class="">
          <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12"  id="content_without_footer">
                <%= yield %>
            </div>
          </div>
          <%= render 'footer' %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
