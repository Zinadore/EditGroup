<%  if resource.errors.any? %>
  $(".modal .modal-body").first().html("<%= j render partial: "admin/#{form_path}", locals: { target: resource } %>");
  $('form:first *:input[type!=hidden]:first').focus();
<% else %>
  $(".modal .modal-body").first().html("<%= j render partial: "admin/#{form_path}" %>");
<% end %>
